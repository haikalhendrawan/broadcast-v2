<%- include('partials/header') %>

<body id="page-top">


    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
        <%- include("partials/sidebar") %>
        <!-- End of Sidebar -->

        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
                <%- include("partials/topbar") %>
                <!-- End of Topbar -->

                <!-- Begin Page Content -->
                <div class="container-fluid">

                    <!-- Page Heading -->
                    <div class="d-sm-flex align-items-center justify-content-between mb-4">
                        <h1 class="h3 mb-0">Dashboard</h1>
                        <!--<a href="#" class="d-none d-sm-inline-block btn btn-sm btn-dark text-light shadow-sm" ><i
                                class="fas fa-download fa-sm text-white-50 mr-2"></i>Generate Report</a>-->
                    </div>
                    <!-- Content Row -->
                    <div class="row">

                        <!-- Broadcast Belum Selesai -->
                        <div class="col-xl-4 col-md-6 mb-4">
                            <div class="card border-left-primary  shadow h-100 py-2">
                                <div class="card-body">
                                    <div class="row no-gutters align-items-center">
                                        <div class="col mr-2">
                                            <div class="text-xs font-weight-bold text-gray-800 text-uppercase mb-1">
                                                Client Status
                                            </div>
                                            <div class="text-xs mb-0 mt-1 row">
                                                <div class="col-md-4" style="gap: 0px; margin: 0px;">
                                                    <p style=" margin: 0px;">Server</p>
                                                    <p style=" margin: 0px;">Pushname</p>
                                                    <p style=" margin: 0px;">Serial</p>
                                                </div>
                                                <div class="col-md-1" >
                                                    <p style=" margin: 0px;">:</p>
                                                    <p style=" margin: 0px;">:</p>
                                                    <p style=" margin: 0px;">:</p>
                                                </div>
                                                <div class="col text-gray-800" id="client-info">

                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-auto">
                                            <i class="fas fa-user fa-2x text-gray-300"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Total Broadcast -->
                        <div class="col-xl-4 col-md-6 mb-4">
                            <div class="card border-left-primary  shadow h-100 py-2">
                                <div class="card-body">
                                    <div class="row no-gutters align-items-center">
                                        <div class="col mr-2">
                                            <div class="text-xs font-weight-bold text-gray-800 text-uppercase mb-1">
                                                Next schedule
                                            </div>
                                            <div class="text-xs mb-0 mt-1 row">
                                                <div class="col-md-4" style="gap: 0px; margin: 0px;">
                                                    <p style=" margin: 0px;">Title</p>
                                                    <p style=" margin: 0px;">Time Due</p>
                                                    <p style=" margin: 0px;">Day status</p>
                                                </div>
                                                <div class="col-md-1" >
                                                    <p style=" margin: 0px;">:</p>
                                                    <p style=" margin: 0px;">:</p>
                                                    <p style=" margin: 0px;">:</p>
                                                </div>
                                                <div class="col text-gray-800" id="schedule-info">

                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-auto">
                                            <i class="fas fa-calendar fa-2x text-gray-300"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Broadcast Selesai -->
                        <div class="col-xl-4 col-md-6 mb-4">
                            <div class="card border-left-primary  shadow h-100 py-2">
                                <div class="card-body">
                                    <div class="row no-gutters align-items-center">
                                        <div class="col mr-2">
                                            <div class="text-xs font-weight-bold text-uppercase mb-1">
                                                Concurrent login
                                            </div>
                                            <div class="h6 mb-0 font-weight-bold text-gray-800" id="broadcast-count"> 
                                                12
                                            </div>
                                            <div class="text-xs mb-0 mt-1 row">
                                                <div class="col-md-4" style="gap: 0px; margin: 0px;">
                                                    <p style=" margin: 0px;">Last IP</p>
                                                </div>
                                                <div class="col-md-1" >
                                                    <p style=" margin: 0px;">:</p>
                                                </div>
                                                <div class="col text-gray-800" id="schedule-info">

                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-auto">
                                            <i class="fas fa-bookmark fa-2x text-gray-300"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- Content Row -->

                    <div class="row">
                        <!-- Area Chart -->
                        <div class="col-xl-7 col-lg-5">
                            <div class="card shadow mb-4">
                                <div class="card-header py-3">
                                    <h6 class="m-0 font-weight-bold ">Broadcast Count</h6>
                                </div>
                                <div class="card-body">
                                    <div class="chart-area">
                                        <canvas id="myAreaChart"></canvas>
                                    </div>
                                    <hr>

                                </div>
                            </div>
                            <input type="hidden" id="broadcastData" value="[]">

                        </div>


                    
                        <!-- Announcement -->
                        <div class="col-xl-5 col-lg-7">
                            <div class="card shadow mb-4">
                                <!-- Card Header - Dropdown -->
                                <div
                                    class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                                    <h6 class="m-0 font-weight-bold ">Contact</h6>
                                    <div class="dropdown no-arrow">
                                        <a class="dropdown-toggle" href="#" role="button" id="dropdownMenuLink"
                                            data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                            <i class="fas fa-ellipsis-v fa-sm fa-fw text-gray-400"></i>
                                        </a>
                                    </div>
                                </div>
                                <!-- Card Body -->
                                <div class="card-body text-gray-800">
                                    <p><b>Group</b></p>
                                    <hr>

                                    <div style="display:flex; flex-direction:column; margin-left: 10px; gap:5px; margin-bottom: 20px;" id="groupContactDisplay">
                                       
                                    </div>
                                    <p><b>User</b></p>
                                    <hr>
                                    <div style="display:flex; flex-direction:column; margin-left: 10px; gap:5px; margin-bottom: 20px;" id="userContactDisplay">
                                        
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- Content Row -->
                    <div class="row">

                    </div>
                    <!-- End of Row-->

                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->

            <!-- Footer -->
            <%- include("partials/footer") %>
            <!-- End of Footer -->
            <%- include("partials/contentfooter") %>
        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <%- include("partials/scrolltotop") %>


    <!-- Bootstrap core JavaScript-->
    <script src="vendor/jquery/jquery.min.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="vendor/jquery-easing/jquery.easing.min.js"></script>

    <!-- Custom scripts for all pages-->
    <script src="js/sb-admin-2.min.js"></script>

    <!-- Page level plugins -->
    <script src="vendor/chart.js/Chart.min.js"></script>

    <!-- Page level custom scripts -->
    <script src="js/broadcastchart.js"></script>

    <!--additional custom sidebar-->
    <script src="js/additionalsidebar.js"></script>

    <!--sweetalert-->
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

    <!--top level js-->
    <script src="/js/pages/mainScript.js"></script>

    <!--main page level js-->
    <script type="module" src="/js/pages/indexPageScript.js"></script>


</body>

</html>
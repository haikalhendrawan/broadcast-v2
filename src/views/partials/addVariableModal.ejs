<!-- Modal -->
<div class="modal fade" id="addVariableModal" tabindex="-1" role="dialog" aria-labelledby="addVariableModal" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Add New Variable</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="form-group">
          <label for="addVariableInput">Variable Name</label>
          <input class="form-control" type="text" name="addVariableInput" id="addVariableInput">
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-success" id="addVariableButton" onclick="addVariable()">Add</button>
        <button type="button" class="btn btn-secondary" data-dismiss="modal" id="resetVariableButton">Cancel</button>
      </div>
    </div>
  </div>
</div>
